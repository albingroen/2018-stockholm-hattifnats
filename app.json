{
  "expo": {
    "name": "Nina",
    "description": "Nina is a application for easily and fun explore job-openings according to your specifications. ",
    "slug": "nina",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "githubUrl": "https://github.com/albingroen/openhack-project",
    "ios": {
      "bundleIdentifier": "com.openhack.nina"
    },
    "android": {
      "package": "com.openhack.nina"
    }
  }
}
